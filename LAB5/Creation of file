import java.io.*;

class L5P5{
   public static void main(String...args) throws IOException{
     File f=new File("Epson.java");
     if(f.exists()){
        System.out.println("Yes exists");
     }
     else{
        f.createNewFile();
        System.out.println("file exists: "+f.exists());
    }
}
}
