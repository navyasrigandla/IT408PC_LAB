import java.util.Scanner;                    // Creating exception method for an odd number
                                            // Throws an exception if given number is not an odd number.

class L4P3 extends Exception{
    public static void getValue(int x){
         if(x%2!=0)
            throw new ArithmeticException("Odd Number");
         else
           System.out.println(x);
    }
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        try{
         int a=sc.nextInt();
         getValue(a);
        }
        catch(Exception e){
         System.out.println(e.getMessage());
        }
  }
}


